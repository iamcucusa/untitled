{
  "name": "@untitled-ds/react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "dev:watch": "concurrently -k -n i18n,web \"pnpm lingui:compile:watch\" \"pnpm dev\"",
    "i18n:refresh": "pnpm lingui:extract && pnpm lingui:compile",
    "lingui:compile": "lingui compile",
    "lingui:compile:watch": "lingui compile --watch",
    "lingui:extract": "lingui extract --clean --overwrite"
  },
  "dependencies": {
    "@untitled-ds/i18n-core": "workspace:*",
    "@untitled-ds/i18n-react": "workspace:*",
    "@untitled-ds/intl-core": "workspace:*",
    "@untitled-ds/presets": "workspace:*",
    "@untitled-ds/styles": "workspace:*",
    "@untitled-ds/tokens": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@lingui/cli": "^5.5.0",
    "@lingui/core": "^5.5.0",
    "@lingui/format-po": "^5.5.0",
    "@lingui/macro": "^5.5.0",
    "@lingui/react": "^5.5.0",
    "@lingui/vite-plugin": "^5.5.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "babel-plugin-macros": "^3.1.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "babelMacros": {
    "lingui": {
      "corePackage": "@lingui/core"
    }
  }
}
